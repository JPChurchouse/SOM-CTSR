	tank_diam  : REAL := 1.8;		// m
	tank_area  : REAL := PI * EXPT(tank_diam, 2) / 4; // m2
	tank_high  : REAL := 2.5;		// m
	
	cip_Tbet   : TIME := T#2H;
	cip_Tlen   : TIME := T#20M;
	
	cip_conc   : REAL := 5;			// mol/m3
	cip_flin   : REAL := 4 /1000;	// m3/s
	norm_conc  : REAL := 10; 		// mol/m3
	norm_flin  : REAL := 2 /1000;	// m3/s
	
	const_grav : REAL := 9.81;		// N/kg
	const_AE   : REAL := 5000;		// K
	const_Rk   : REAL := 578 *1000;	// m3/mol/s
	const_rho  : REAL := 1000;		// kg/m3
	const_Kwal : REAL := 600;		// J/s/K/m2
	const_cp   : REAL := 4.18*1000; // J/kg/K
	const_Hr   : REAL := 45  *1000;	// J/mol
	
END_VAR]]></Declaration>
