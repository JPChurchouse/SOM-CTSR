  <GVL Name="GVL" Id="{a7ab9a1e-0490-4ced-b6de-04654a068e1f}">
    <Declaration><![CDATA[{attribute 'qualified_only'}
VAR_GLOBAL
	
	// INPUTS
	// Outlet valve position (0.00-1.00)
	valve_oule : REAL := 0;
	// Input steam tempuratue (there are min/max)
	steam_temp : REAL := 50;
	
	// MAIN VARIABLES
	high_now   : REAL := 2.2;
	temp_now   : REAL := 80;
	conc_now   : REAL := 1.0;
	
	volPC_now  : REAL := 0;
	
	// Flows - volume per second
	flow_in    : REAL := 2 /1000;	// m3/s
	flow_out   : REAL := 0;			// m3/s
	
	// Concentration in
	conc_in    : REAL := 10;		// mol/m3
	
	temp_wall  : REAL := 25;
	
END_VAR

VAR_GLOBAL CONSTANT
	
	time_step  : REAL := 2;			// s
	
	temp_inle  : REAL := 25;		// °c
